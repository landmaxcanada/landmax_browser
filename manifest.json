{
  "manifest_version": 2,
  "name": "Landmax Real Estate Browser",
  "short_name": "Landmax Web",
  "description": "A Chrome Browser optimized for facilitiating Real Estate Transactions in Canada.",
  "version": "0.0.2",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },

  "permissions": [
    "webview", "geolocation","unlimitedStorage", "storage", "syncFileSystem", "fileSystem",
    "contextMenus", "clipboardRead","http://*/*"
    ]
}
